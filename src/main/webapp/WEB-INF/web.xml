<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <!--defining ServletConfig initialization parameters which are immutable once app starts-->
  <context-param>
         <!-- both name and value are of string type -->
         <param-name>appName</param-name>
         <param-value>FeedbackApp</param-value>
  </context-param>
  
  <context-param>
         <!-- both name and value are of string type -->   
         <param-name>appVersion</param-name>
         <param-value>2.1.0</param-value>
  </context-param>
  
  <servlet>
        
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>cookies.ServletOne</servlet-class>
        <init-param>
            <param-name>ServletName</param-name>
            <param-value>FirstServlet</param-value>
        </init-param>
  
 </servlet>
 
 <servlet-mapping>
 <servlet-name>FirstServlet</servlet-name>
 <url-pattern>/ServletOne</url-pattern>
 </servlet-mapping>
 
 
  
  
  
</web-app>
